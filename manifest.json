{
	"manifest_version": 3,
	"name": "My Extension",
	"version": "1.0",
	"description": "Change background color",
	"permissions": [
		"scripting",
		"activeTab"
	],
	"host_permissions": [
		"http://*/*",
		"https://*/*"
	],
	"action": {
		"default_popup": "popup.html"
	},
	"background": {
		"service_worker": "background.js"
	}
}
